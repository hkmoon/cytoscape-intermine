[{
  "selector": "node",
  "style": {
    "content": "data(label)"
  }
}, {
  "selector": ":selected",
  "style": {
    "text-outline-color": "black",
    "background-color": "gold",
    "border-color": "gold",
    "border-width": "5px",
    "border-style": "solid"
  }
}, {
  "selector": "edge",
  "style": {
    "width": "2px"
  }
}, {
  "selector": "edge[interactionType@=\"genetic\"]",
  "style": {
    "line-color": "#2c79be"
  }
}, {
  "selector": "edge[interactionType@=\"physical\"]",
  "style": {
    "line-color": "red"
  }
}, {
  "selector": ".genetic",
  "style": {
    "background-color": "#225f95",
    "shape":"rectangle"
  }
}, {
  "selector": ".physical",
  "style": {
    "background-color": "#c00",
    "shape":"ellipse"
  }
}, {
  "selector": ".both",
  "style": {
    "background-color": "#77304B",
    "shape":"roundrectangle"
  }
}]
